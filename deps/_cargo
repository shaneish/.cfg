cargo install --locked bat
cargo install fd-find
cargo install --locked jaq
cargo install --locked yazi-fm yazi-cli
cargo install rust-parallel
cargo install fselect
cargo isntall xh
cargo install coreutils
cargo install bhop
cargo install xsv
cargo install eza
cargo install ouch
cargo install choose
cargo install tealdeer
cargo install onefetch --force
cargo install starship
cargo install tree-rs
cargo install --locked typst-cli
cargo install --locked --git https://github.com/asciinema/asciinema/tree/v3.0.0-rc.5 # asciicinema
cargo install --git https://github.com/asciinema/agg/tree/v1.5.0 #asciicinema gif maker

_alias eza ll
_alias fselect fs
_alias rust-parallel rp
_alias tree-rs tree
